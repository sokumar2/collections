{
    "type": "library",
    "require-dev": {
        "phpunit/phpunit": "^12.4"
    },
    "autoload": {
        "psr-4": {
            "Collection\\": "src"
        }
    },
    "scripts": {
        "test": [
            "./vendor/bin/phpunit tests"
        ],
        "coverage": [
            "XDEBUG_MODE=coverage ./vendor/bin/phpunit --coverage-clover tests/report/clover.xml tests"
        ]
    }
}
